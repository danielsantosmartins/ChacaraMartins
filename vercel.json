{
    "version": 2,
    "builders": [{
      "name": "python",
      "command": "python manage.py collectstatic --noinput",
      "working_dir": "setup"
    }],
    "routes": [{
      "src": "/static/*",
      "dest": "public/static"
    }],
    "functions": [{
      "name": "main",
      "source": "setup/wsgi.py",
      "handler": "wsgi.handler",
      "runtime": "python3.8",
      "environment": {
        "DJANGO_SETTINGS_MODULE": "setup.settings.production"
      }
    }]
  }
